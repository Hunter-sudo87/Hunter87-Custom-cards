--Cyber Barrierenlaser Dragon
--
local s,id=GetID()
function s.initial_effect(c)
	c:EnableReviveLimit()
	--Synchro Summon Procedure
	Synchro.AddProcedure(c,nil,1,1,Synchro.NonTuner(nil),1,99,s.matfilter)
end
function s.matfilter(c,scard,sumtype,tp)
	return c:IsCode(CARD_CYBER_DRAGON,scard,sumtype,tp) or (c:IsMonster() and c:ListsCode(CARD_YUBEL,scard,sumtype,tp)
end
